# SportApp Environment Configuration
# Copy this file to .env and update with your values

# Development/Production Environment
NODE_ENV=development

# API Configuration (if needed in future)
API_BASE_URL=https://api.sportapp.com
API_KEY=your_api_key_here

# Database Configuration (if needed in future)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=sportapp_db
DB_USER=your_db_user
DB_PASSWORD=your_db_password

# React Native Configuration
EXPO_DEBUG=true
FLIPPER_DISABLE=false

# Android Configuration
ANDROID_SDK_ROOT=/Users/your_username/Library/Android/sdk

# iOS Configuration (macOS only)
IOS_SIMULATOR_UDID=your_simulator_udid

# Build Configuration
BUILD_NUMBER=1
VERSION_CODE=1
VERSION_NAME=1.0.0

# Analytics (if needed in future)
ANALYTICS_KEY=your_analytics_key
CRASHLYTICS_KEY=your_crashlytics_key

# Push Notifications (if needed in future)
FCM_SERVER_KEY=your_fcm_server_key
APNS_KEY=your_apns_key