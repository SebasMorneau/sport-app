# React Native Environment Configuration
# Copy this file to .env and update the values

# API Configuration
API_URL=http://localhost:3500/api
REACT_APP_API_URL=http://localhost:3500/api

# Development Settings
DEV_API_URL_IOS=http://localhost:3500/api
DEV_API_URL_ANDROID=http://10.0.2.2:3500/api

# Production API (Required for production builds)
# PROD_API_URL=https://api.yourdomain.com/api

# Feature Flags
ENABLE_CRASH_REPORTING=false
ENABLE_ANALYTICS=false
ENABLE_PERFORMANCE_MONITORING=false

# Third-party Services (Optional)
# SENTRY_DSN=your-sentry-dsn
# BUGSNAG_API_KEY=your-bugsnag-key
# MIXPANEL_TOKEN=your-mixpanel-token

# Social Authentication (Optional)
# GOOGLE_CLIENT_ID=your-google-client-id
# FACEBOOK_APP_ID=your-facebook-app-id
# APPLE_SERVICE_ID=your-apple-service-id

# Push Notifications (Optional)
# FCM_SENDER_ID=your-fcm-sender-id
# ONESIGNAL_APP_ID=your-onesignal-app-id

# App Configuration
APP_NAME=SportApp
APP_BUNDLE_ID=com.sportapp
APP_VERSION=1.0.0

# Build Configuration
# CODE_PUSH_DEPLOYMENT_KEY_IOS=your-ios-deployment-key
# CODE_PUSH_DEPLOYMENT_KEY_ANDROID=your-android-deployment-key

# Map Services (Optional)
# GOOGLE_MAPS_API_KEY_IOS=your-ios-maps-key
# GOOGLE_MAPS_API_KEY_ANDROID=your-android-maps-key

# Storage Services (Optional)
# AWS_S3_BUCKET=your-s3-bucket
# AWS_S3_REGION=your-s3-region
# CLOUDINARY_CLOUD_NAME=your-cloudinary-name